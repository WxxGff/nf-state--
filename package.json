{
  "name": "@naturefw/nf-state",
  "version": "0.4.2",
  "description": "类似于 pinia 的轻量级状态管理，直接使用 Vue3 的 reactive 实现,TS版。",
  "keywords": [
    "vue3",
    "nf-state",
    "state",
    "vuex",
    "pinia"
  ],
  "files": [ "dist" ],
  "main": "./dist/nf-state.umd.js",
  "module": "./dist/nf-state.es.js",
  "exports": {
    ".": {
      "import": "./dist/nf-state.es.js",
      "require": "./dist/nf-state.umd.js"
    }
  },
  "types": "./dist/nf-state.d.ts",
  "private": false,
  "license": "MIT",
  "auther": "jin yang (jyk). Email: jyk0013@163.com",
  "scripts": {
    "dev": "vite",
    "build": "vite build --mode project",
    "lib": "vite build --mode lib",
    "dts": "vue-tsc --declaration --emitDeclarationOnly ",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^16.11.27",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.5.0",
    "eslint-plugin-vue": "^8.2.0",
    "typescript": "~4.6.3",
    "@element-plus/icons-vue": "^1.1.4",
    "@naturefw/ui-elp": "^0.1.18",
    "pinia": "^2.0.14",
    "vite": "^2.9.5",
    "vue": "^3.2.33",
    "vue-tsc": "^0.34.7"
  }
}
